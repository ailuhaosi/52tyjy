  //购物车功能：选题与移除的点击功能，并在Cart_Modal中添加已购内容
  $(document).on('click', '#my_Questions_li>.QuestionView>.question-block .J_add_remove', function () {

    let Xd_Chid = get_Xd_Chid();
    let myXd = Xd_Chid.myXd;
    let myChid = Xd_Chid.myChid;

    let cart_num = parseInt($('.rightside .divnum').text());
    let str_btn_content = $(this).text();
    let data_no = $(this).parents('.QuestionView').attr('data-no');
    let question_channel_type = $(this).parents('.QuestionView').find('.q-msg span').eq(0).text();
    question_channel_type = question_channel_type.substring(3);

    let Cart_Modal_contents = localStorage.getItem("Cart_Modal_contents");
    //本地存储为空时，购物车没商品的情况。必须这样分，不然JSON.parse没法用。
    //alert(Cart_Modal_contents=='{}');
    if ((Cart_Modal_contents == 'undefined') || (Cart_Modal_contents == 'null') || (Cart_Modal_contents == null) || (Cart_Modal_contents == undefined) || (Cart_Modal_contents == {}) || (Cart_Modal_contents == '{}')) {
      if ($('#Cart_Modal .modal-body>center').children('p').text() == '您的购物车，空空如也！') {
        $('#Cart_Modal .modal-body>center').html('<p>' + question_channel_type + '&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;' + 1 + '题' + '</p>');
        //$('#Cart_Modal .modal-body>center').append("<div style='display:none'" + "id=" + question_channel_type + ">" + data_no + '</div>');
        if (str_btn_content == '+ 选题') {
          $(this).addClass('active');
          $(this).text('- 移除');
        }
        alert('首次加入题目' + '现在' + question_channel_type + '的数量=' + 1);
        let Cart_Modal_contents = {};
        Cart_Modal_contents[question_channel_type] = '1';
        Cart_Modal_contents[question_channel_type + "的data_no"] = new Array(data_no);
        Cart_Modal_contents['myXd'] = myXd;
        Cart_Modal_contents['myChid'] = myChid;

        //将购物车信息提交，保存到，对应用户数据库中
        post_cart_save(Cart_Modal_contents);

        Cart_Modal_contents = JSON.stringify(Cart_Modal_contents);//将JSON对象转化成字符串       
        localStorage.setItem("Cart_Modal_contents", Cart_Modal_contents);//用localStorage保存转化好的的字符串
        $('.rightside .divnum').text('' + 1);

      }
    }
    //本地存储为不空时，购物车有商品的情况。
    else {
      Cart_Modal_contents = JSON.parse(Cart_Modal_contents);
      let arr_cart;
      for (let key in Cart_Modal_contents) {
        //遍历所有的key，取出值为数组的，即为每个题型的data_no内容
        if ((key.indexOf('的data_no') != -1) && (typeof (Cart_Modal_contents[key]) != "string")) {
          arr_cart = Cart_Modal_contents[key];
          //alert(arr_cart);
        }
      }
      //遍历每个题型的data_no数组
      //alert(arr_cart==undefined);
      if (arr_cart != undefined) {
        data_no_Arry_by_tixing:
        for (let j = 0, len = arr_cart.length; j < len; j++) {//TODO:
          //需要判断，当前选择的题目是否已经放在了购物车，如果已经在购物车了，则不能添加了
          if ((data_no == arr_cart[j]) && (str_btn_content == '+ 选题')) {
            alert('该题目已经加入购物车了,请勿重复加入。');
            break data_no_Arry_by_tixing;
            //j = len+1000;

          }
          //遍历完数组，如果当前题目没有放在购物车里，则则执行购物车主功能程序
          else if (j == len - 1) {
            //添加题目到购物车
            if (str_btn_content == '+ 选题') {
              cart_num++;
              $(this).addClass('active');
              $(this).text('- 移除');
              let now_p_num = 0;
              let my_flag_break_each = true;
              $('#Cart_Modal .modal-body>center p').each(function () {
                now_p_num++;
                let now_text = $(this).text();
                //购物车已经存在该商品，该商品数量加1。
                if (now_text.indexOf(question_channel_type) != -1) {
                  let Cart_Modal_contents = localStorage.getItem("Cart_Modal_contents");
                  Cart_Modal_contents = JSON.parse(Cart_Modal_contents);
                  let now_num = parseInt(Cart_Modal_contents[question_channel_type]) + 1;
                  Cart_Modal_contents[question_channel_type] = '' + now_num;
                  Cart_Modal_contents[question_channel_type + "的data_no"].push(data_no);///
                  Cart_Modal_contents['myXd'] = myXd;
                  Cart_Modal_contents['myChid'] = myChid;
                  Cart_Modal_contents = JSON.stringify(Cart_Modal_contents);
                  localStorage.setItem("Cart_Modal_contents", Cart_Modal_contents);

                  $(this).html(question_channel_type + '&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;' + now_num + '题');
                  //$('#Cart_Modal .modal-body>center').append("<div style='display:none'" + "id=" + question_channel_type + ">" + data_no + '</div>');
                  alert('已包含该题型,该题型数量加1。' + '现在 ' + question_channel_type + ' 的数量=' + now_num);
                  return my_flag_break_each = false;//注意jquery的each循环不能用break与 continue 用return true;而且必须有外部flag。
                }
                //购物车加入了新商品，新商品数量设为1。
                else {
                  if ($('#Cart_Modal .modal-body>center').children('p').length == now_p_num) {
                    $('#Cart_Modal .modal-body>center').append('<p>' + question_channel_type + '&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;' + 1 + '题' + '</p>');
                    //$('#Cart_Modal .modal-body>center').append("<div style='display:none'" + "id=" + question_channel_type + ">" + data_no + '</div>');
                    alert('这道题目是新题型，第一次加入该题型。' + '现在' + question_channel_type + '的数量=' + 1);
                    let Cart_Modal_contents = localStorage.getItem("Cart_Modal_contents");
                    Cart_Modal_contents = JSON.parse(Cart_Modal_contents);
                    Cart_Modal_contents[question_channel_type] = '1';
                    Cart_Modal_contents[question_channel_type + "的data_no"] = new Array(data_no);
                    Cart_Modal_contents['myXd'] = myXd;////FIXME:
                    Cart_Modal_contents['myChid'] = myChid;////FIXME:
                    Cart_Modal_contents = JSON.stringify(Cart_Modal_contents);
                    localStorage.setItem("Cart_Modal_contents", Cart_Modal_contents);
                  }
                }
              });
            }


            //从购物车移除题目
            else if (str_btn_content == '- 移除') {
              cart_num--;
              $(this).removeClass('active');
              $(this).text('+ 选题');

              let Cart_Modal_contents = localStorage.getItem("Cart_Modal_contents");
              Cart_Modal_contents = JSON.parse(Cart_Modal_contents);
              let my_flag_break_each = true;
              $('#Cart_Modal .modal-body>center p').each(function () {

                let now_text = $(this).text();
                if (now_text.indexOf(question_channel_type) != -1) {
                  //该题型 找到的数量为1
                  if (Cart_Modal_contents[question_channel_type] == '1') {

                    $(this).html('');

                    //$('#Cart_Modal .modal-body>center>div:contains('+data_no+')').remove();
                    delete Cart_Modal_contents[question_channel_type];
                    delete Cart_Modal_contents[question_channel_type + "的data_no"];
                    if (Object.keys(Cart_Modal_contents).length == 2) {//如果本地存储信息只剩'myXd'、'myChid'那么说明购物车空了
                      delete Cart_Modal_contents['myXd'];
                      delete Cart_Modal_contents['myChid'];
                      $(this).text('您的购物车，空空如也！');
                    }
                    //let delet_data_no_index = Cart_Modal_contents[question_channel_type + "的data_no"].indexOf(data_no);
                    //Cart_Modal_contents[question_channel_type + "的data_no"].splice(delet_data_no_index, 1);
                    Cart_Modal_contents = JSON.stringify(Cart_Modal_contents);
                    localStorage.setItem("Cart_Modal_contents", Cart_Modal_contents);
                    alert('最后一道' + question_channel_type + '成功删除');
                    return my_flag_break_each = false;
                  }
                  //该题型 找到的数量大于1
                  else if (Cart_Modal_contents[question_channel_type] != '1') {

                    let q_c_t_num = parseInt(Cart_Modal_contents[question_channel_type]);
                    q_c_t_num--;
                    Cart_Modal_contents[question_channel_type] = '' + q_c_t_num;
                    $(this).html(question_channel_type + '&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;' + q_c_t_num + '题');
                    alert('删除成功，现在' + question_channel_type + '还剩' + q_c_t_num + '题');
                    //$('#Cart_Modal .modal-body>center>div:contains(' + data_no + ')').remove();
                    let delet_data_no_index = Cart_Modal_contents[question_channel_type + "的data_no"].indexOf(data_no)
                    Cart_Modal_contents[question_channel_type + "的data_no"].splice(delet_data_no_index, 1);
                    Cart_Modal_contents = JSON.stringify(Cart_Modal_contents);
                    localStorage.setItem("Cart_Modal_contents", Cart_Modal_contents);

                  }
                }
              });

            }
            $('.rightside .divnum').text('' + cart_num);
            //将购物车信息本地存储，以便跨页访问
            let Cart_Modal_contents = localStorage.getItem("Cart_Modal_contents");
            Cart_Modal_contents = JSON.parse(Cart_Modal_contents);
            //将购物车信息提交，保存到，对应用户数据库中
            post_cart_save(Cart_Modal_contents);
          }
        }
      }
      //}
    }

  })

  //传入json形式的购物车信息，存到数据库中//后台根据网页头信息中的cookie解码后的username来判断存到哪个用户下
  function post_cart_save(Cart_Modal_contents) {

    //var currentToken = getCookie('access-token');
    //'access-token': currentToken
    $.ajax({
      type: 'post',
      url: '//www.52tyjy.com/cart_save',
      dataType: 'json',
      headers: {
        'Content-Type': 'application/x-www-form-urlencoded;charset=utf-8'
      },
      async: true,//false
      data: {
        Cart_Modal_contents: Cart_Modal_contents
      },
      success: function (data) {
        if (data.type) {
          alert('购物车信息保存成功');
        }
        if (data.type == false) {
          alert(data.data);
        }
