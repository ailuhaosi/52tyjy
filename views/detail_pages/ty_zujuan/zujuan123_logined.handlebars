                //以下函数用于修改input的值，该值用于ajax给后台，实现局部刷新题目
                function mypageclick(myvalue) {
                  let num;
                  let i = 0;
                  if (myvalue.length > 0) {
                    for (i; i < 7; i++) {
                      if (parseInt(myvalue) >= 3) {
                        //alert('' + myvalue)
                        if ((i === 0) || (i === 2)) num = parseInt(myvalue) - 1;
                        if (i === 1) num = parseInt(myvalue) - 2;
                        if (i === 3) num = parseInt(myvalue);
                        if ((i === 4) || (i === 6)) num = (parseInt(myvalue) + 1);
                        if (i === 5) num = (parseInt(myvalue) + 2);
                        $('.pagenum>a').eq(0).show();
                        $('.pagenum>a').removeClass("current");
                        $('.pagenum>a').eq(3).addClass("current");
                      }

                      else if (parseInt(myvalue) < 3) {
                        //alert('' + myvalue)
                        if (parseInt(myvalue) === 2) {
                          if (i === 0) num = 1;
                          if (i === 1) num = 1;
                          if (i === 2) num = 2;
                          if (i === 3) num = 3;
                          if (i === 4) num = 4;
                          if (i === 5) num = 5;
                          if (i === 6) num = 3;
                          $('.pagenum>a').eq(0).show();
                          $('.pagenum>a').removeClass("current");
                          $('.pagenum>a').eq(2).addClass("current");
                        }
                        if (parseInt(myvalue) === 1) {
                          //if (i === 0) num = 1;
                          if (i === 1) num = 1;
                          if (i === 2) num = 2;
                          if (i === 3) num = 3;
                          if (i === 4) num = 4;
                          if (i === 5) num = 5;
                          if (i === 6) num = 2;
                          $('.pagenum>a').eq(0).hide();
                          $('.pagenum>a').removeClass("current");
                          $('.pagenum>a').eq(1).addClass("current");
                        }
                      }
                      let mystr = '' + num
                      $('.pagenum>a').eq(i).attr('data-page', mystr);
                      if ((i != 0) && (i != 6)) { $('.pagenum>a').eq(i).text(mystr); }
                    }
