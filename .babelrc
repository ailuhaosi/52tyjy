{
  "presets": [
    [
      "@babel/preset-env",
      {"modules": false,
        "targets": {
          "node": "current",
          "browsers": [
            "IE >= 9"
          ]
        },
        "useBuiltIns": true
      }
    ]
  ],

  "plugins": [
    "@babel/plugin-transform-object-assign", 
    ["@babel/plugin-transform-runtime",
      {
        "corejs": 2,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ]
  ]

}