{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "启动node调试:直接启动模式",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/index.js"
        },
        {
            "name": "启动node调试:附加到端口模式",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "localRoot": "${workspaceRoot}",
        },
        {
            "name": "启动chrome调试:直接启动模式",
            "type": "chrome",
            "request": "launch",
            "url": "https://www.52tyjy.com",
            "webRoot": "${workspaceRoot}",//
            "runtimeExecutable":"C:/Program Files (x86)/Google/Chrome/Application/chrome.exe"
        },
        {
            "name": "启动chrome调试:附加到端口模式",
            "type": "chrome",
            "request": "attach",
            "port": 3000,
            "webRoot": "${workspaceRoot}"
        }
        
    ]
}